# This file was created for <%= node.name %>
# by Chef
# Manual changes will be lost
input {
  file {
    path => ["/opt/jetty/logs/logback.log"]
    codec => json
    type => application_logs
    <% if node['logstash']['agent']['debug'] -%>
    debug => true
    <% end -%>
  }
}

output {
  <% if node['logstash']['agent']['debug'] -%>
  stdout { }
  <% end -%>

  file {
    path => "/opt/jetty/logs/logstash.log"
  }
}